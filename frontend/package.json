{
  "dependencies": {
    "@react-native-picker/picker": "^2.11.0",
    "dotenv": "^16.4.7",
    "expo": "^52.0.42",
    "fastify": "^5.2.2",
    "fastify-plugin": "^5.0.1",
    "knex": "^3.1.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "react-dom": "18.3.1",
    "react-native": "0.76.8",
    "react-native-web": "^0.19.13",
    "sequelize": "^6.37.6"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npx expo start\"",
    "server": "ts-node ../src/server/server.ts",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "expo": {
    "name": "GameOn",
    "slug": "gameon",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.gameon.app"
    },
    "android": {
      "package": "com.gameon.app"
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "react-native-web"
          ]
        }
      }
    }
  }
}
